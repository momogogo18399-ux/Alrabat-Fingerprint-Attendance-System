{
  "version": 2,
  "builds": [
    {
      "src": "web_app.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "50mb", 
        "runtime": "python3.11",
        "includeFiles": "**"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "FLASK_ENV": "production"
  },
  "functions": {
    "web_app.py": {
      "maxDuration": 30
    }
  }
}